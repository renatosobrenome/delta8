algoritmo "Soma até Exceder 15"
var
   num1, num2, soma: inteiro
inicio
   repita
      escreva("Digite o primeiro valor: ")
      leia(num1)
      escreva("Digite o segundo valor: ")
      leia(num2)
      soma <- num1 + num2
   ate soma > 15

   escreva("Soma maior que 15! Programa encerrado.")
fimalgoritmo